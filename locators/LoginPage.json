{
  "url": "https://laboratorio.qaminds.com/index.php?route=account/login",
  "locators": {
    "email_input": {
      "by": "ID",
      "value": "input-email"
    },
    "password_input": {
      "by": "ID",
      "value": "input-password"
    },
    "login_btn": {
      "by": "XPATH",
      "value": "//input[@value=\"Login\"]"
    },
    "warning_message": {
      "by": "CLASS_NAME",
      "value": "alert-danger"
    },
    "forgotten_password_linktext": {
      "by": "XPATH",
      "value": "//div[@class='form-group']//a"
    },
    "continue_btn": {
      "by": "XPATH",
      "value": "//input[@value='Continue']"
    }
  }
}
